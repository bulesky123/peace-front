.add-house-container {
  padding: 0 40rpx;
}
.add-house-container .input-item {
  display: flex;
  height: 130rpx;
  justify-content: space-between;
  align-items: center;
  border-bottom: 2rpx solid #EFEFEF;
}
.add-house-container .input-item .input-lable {
  font-size: 34rpx;
  font-family: PingFangSC-Regular, PingFang SC;
  font-weight: 400;
  color: #05203D;
  line-height: 48rpx;
}
.add-house-container .input-item .input-test {
  font-size: 34rpx;
  font-family: PingFangSC-Regular, PingFang SC;
  font-weight: 400;
  color: #A1A1A1;
  line-height: 48rpx;
  text-align: right;
}
.house-list {
  display: flex;
  height: 130rpx;
  justify-content: space-between;
  align-items: center;
  background: #F4F4F4;
  padding: 0 40rpx;
  margin-bottom: 32rpx;
}
.house-list .house-head {
  font-size: 36rpx;
  font-family: PingFangSC-Medium, PingFang SC;
  font-weight: 500;
  color: #05203D;
  line-height: 50rpx;
}
.house-list .house-head .tips {
  font-size: 34rpx;
  font-family: PingFangSC-Regular, PingFang SC;
  font-weight: 400;
  color: #A1A1A1;
  line-height: 48rpx;
}
.house-list .house-num {
  font-size: 32rpx;
  font-family: PingFangSC-Regular, PingFang SC;
  font-weight: 400;
  color: #F77C10;
  line-height: 44rpx;
}
.house-container {
  padding: 0 40rpx;
}
.house-container .fold-title {
  padding-bottom: 40rpx;
}
.house-container .fold-container .flod-item {
  display: inline-block;
  width: calc((100% - 96rpx) / 5);
  box-sizing: content-box;
  padding: 10rpx 0;
  text-align: center;
  margin-right: 24rpx;
  margin-bottom: 32rpx;
  background: #2154F4;
  border-radius: 8rpx;
  font-size: 40rpx;
  font-family: PingFangSC-Medium, PingFang SC;
  font-weight: 500;
  color: #FFFFFF;
  line-height: 56rpx;
  color: #fff;
}
.house-container .fold-container .flod-item:nth-child(5n) {
  margin-right: 0;
}
.house-container .fold-container .add {
  background: #fff;
  border: 2rpx solid #1846C1;
}
.house-container .add-house-btn {
  padding: 40rpx 0;
}

