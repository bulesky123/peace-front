.house {
  padding: 40rpx;
}
.add-house-btn {
  position: fixed;
  bottom: 60rpx;
  left: 40rpx;
  right: 40rpx;
}
.empty {
  text-align: center;
  padding-top: 140rpx;
}
.empty .empty-img {
  width: 400rpx;
  height: 400rpx;
}
.empty .empty-tips {
  margin-top: 24rpx;
  font-size: 34rpx;
  font-family: PingFangSC-Regular, PingFang SC;
  font-weight: 400;
  color: #A1A1A1;
  line-height: 48rpx;
}
.house-item {
  display: flex;
  justify-content: space-between;
  align-items: center;
  padding: 46rpx 32rpx;
  background: #F4F4F4;
}
.house-item .item-left .item-title {
  font-size: 36rpx;
  font-family: PingFangSC-Medium, PingFang SC;
  font-weight: 500;
  color: #05203D;
  line-height: 50rpx;
}
.house-item .item-left .item-tips {
  margin-top: 20rpx;
  font-size: 34rpx;
  font-family: PingFangSC-Regular, PingFang SC;
  font-weight: 400;
  color: #6E717D;
  line-height: 48rpx;
}
.house-item .item-left .item-tips .num {
  color: #F77C10;
}
.house-item .item-right {
  text-align: right;
}
.house-item .item-right .item-opts {
  color: #000;
  padding-right: 20rpx;
}
.house-item .item-right .item-zz {
  margin-top: 20rpx;
  font-size: 32rpx;
  font-family: PingFangSC-Regular, PingFang SC;
  font-weight: 400;
  color: #F77C10;
  line-height: 44rpx;
}

